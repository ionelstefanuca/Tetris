;strategie de punere a unei piese pe tabla
; iau fiecare linie, de la 1 la 22 si incerc sa pun piesa

(defrule SetLinieDeVerificatPiesaAAAA1
(declare (salience 40))
	?AAA<-(genereazaRandomPiese)
	?AA<-(linieNr0 ?nr)
	?ASA<-(linieNr1 ?nr1)
	?ins<-(object (is-a linie)(name ?name)(linieNr ?nr)(p0 -)(p1 -)(p2 -)(p3 -))
	?a <- (meniu 4)
	?b <- (meniu)	
	?Acs<-(piesaSeletata a)
	=>
	;prima linie este 0
	(send ?ins put-p0 A) 
	(send ?ins put-p1 A)
	(send ?ins put-p2 A)
	(send ?ins put-p3 A)
	(retract ?a)
	(retract ?b)
	(assert (afisareTabela))
	(retract ?AAA)
	(retract ?AA)
	(retract ?Acs)
	(retract ?ASA)
	(assert (eliminaLiniiPline))
	(bind ?*scor* (+ ?*scor* 7))
)

