	?a<-(eliminaLiniiPline)
	=>
	(retract ?a)
)


;;vom scrie functie pt a interschimba liniile intre ele
(defrule EliminaLiniePlinaRegula
(declare (salience 20))
	?AA<-(EliminaLiniePlina ?a ?b)
	?inst1<-(object (is-a linie)(linieNr ?a)) ; instanta care trebuie eliminata
	?inst2<-(object (is-a linie)(p0 ?A)(p1 ?B)(p2 ?C)(p3 ?D)(p4 ?E)(p5 ?F)(p6 ?G)(p7 ?H)(p8 ?I)(p9 ?J)(linieNr ?b))  ;instanta care "cade" cu un rand
	=>
	(send ?inst1 put-p0 ?A) 
	(send ?inst1 put-p1 ?B) 
	(send ?inst1 put-p2 ?C)
	(send ?inst1 put-p3 ?D)
	(send ?inst1 put-p4 ?E)
	(send ?inst1 put-p5 ?F)
	(send ?inst1 put-p6 ?G)
	(send ?inst1 put-p7 ?H)
	(send ?inst1 put-p8 ?I)
	(send ?inst1 put-p9 ?J)
	
	(send ?inst2 put-p0 -) 
	(send ?inst2 put-p1 -) 
	(send ?inst2 put-p2 -) 
	(send ?inst2 put-p3 -) 
	(send ?inst2 put-p4 -) 
	(send ?inst2 put-p5 -) 
	(send ?inst2 put-p6 -) 
	(send ?inst2 put-p7 -) 
	(send ?inst2 put-p8 -) 
	(send ?inst2 put-p9 -) 
	(retract ?AA)
	(assert(EliminaLiniePlina ?b (+ ?b 1)))
	
	;(printout t "Elimina "?b " " (+ ?b 1) crlf)
)

(defrule EliminaLiniePlinaRegulaCurataEticheta
(declare (salience 15))
	?AA<-(EliminaLiniePlina ?a ?b)
	=>
	(retract ?AA)
	(assert (afisareTabela))
)

;daca linia este mai mare ca 22 inseamana ca jocul s-a sfarsit
(defrule GameOver
(declare (salience 1000))
	(genereazaRandomPiese)
	?AA<-(linieNr0 ?nr)
	(test (= ?nr ?*LiniiTabla*))
	=>
	(printout t "Game Over !" crlf)
	(halt)
)






