;afisam scorul si nivelul de dificultate
(defrule afisareInfoJoc
(declare (salience 150))
	(afisareTabela)
	=>
	(printout t crlf"Dificultate " ?*dificultate* " din 3" crlf )
	(printout t "Scorul "?*scor* crlf)
	(printout t "Linii completate: "?*LiniiCompletate* crlf)
	(assert (linieNr ?*LiniiTabla*))
)

;afisam tabela
(defrule afisareTabela
(declare (salience 140))
	(afisareTabela)
	?AA<-(linieNr ?nr)
	?ins<-(object (is-a linie)(p0 ?a)(p1 ?b)(p2 ?c)(p3 ?d)(p4 ?e)(p5 ?f)(p6 ?g)(p7 ?h)(p8 ?i)(p9 ?j)(name ?name)(linieNr ?nr))
	=>
	(printout t ?a " " ?b " "?c " " ?d " " ?e " " ?f " " ?g " " ?h " " ?i " " ?j "     " ?nr crlf)
	(assert (linieNr (- ?nr 1)))
	(retract ?AA)
)


(defrule TreciLaNivelul2
(declare (salience 11))
	(not(TreciLaNivelul2))
	(afisareTabela)
	(test (> ?*scor* ?*ScorPentruLevel2*))
	(test (= 1 ?*dificultate*))
	=>
	(bind ?*dificultate* 2) 
	(bind ?*LiniiTabla* 15) 
	(assert (TreciLaNivelul2))
	(assert (CurataTabla2))
)

(defrule TreciLaNivelul3
(declare (salience 11))
	(not(TreciLaNivelul3))
	(afisareTabela)
	(test (> ?*scor* ?*ScorPentruLevel3*))
	(test (= 2 ?*dificultate*))
	=>
	(bind ?*dificultate* 3) 
	(bind ?*LiniiTabla* 10) 
	(assert (TreciLaNivelul3))
	(assert (CurataTabla3))
)

(defrule CurataTabla2
(declare (salience 10))
	(CurataTabla2)
	?ins<-(object (is-a linie))
	=>
	(send ?ins put-p0 -) 
	(send ?ins put-p1 -) 
	(send ?ins put-p2 -) 
	(send ?ins put-p3 -) 
	(send ?ins put-p4 -) 
	(send ?ins put-p5 -) 
	(send ?ins put-p6 -) 
	(send ?ins put-p7 -) 
	(send ?ins put-p8 -) 
	(send ?ins put-p9 -) 
)

(defrule CurataTabla2StergeEticheta
(declare (salience 10))
	?a<-(CurataTabla2)
	=>
	(retract ?a)
)


(defrule CurataTabla3
(declare (salience 10))
	(CurataTabla3)
	?ins<-(object (is-a linie))
	=>
	(send ?ins put-p0 -) 
