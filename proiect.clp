(deffacts eliminaLiniiPline (eliminaLiniiPline))
;vom verifica daca am resusit sa completam o linie, in caz pozitiv vom elimina linia completata
(defrule VerificaDacaExistaVroLinieCompletata
	(declare (salience 2))
	(object (is-a linie)(p0 ?a)(p1 ?b)(p2 ?c)(p3 ?d)(p4 ?e)(p5 ?f)(p6 ?g)(p7 ?h)(p8 ?i)(p9 ?j)(linieNr ?A))
	(eliminaLiniiPline)
	(test(<>(str-compare ?a -)0))
	(test(<>(str-compare ?b -)0))
	(test(<>(str-compare ?c -)0))
	(test(<>(str-compare ?d -)0))
	(test(<>(str-compare ?e -)0))
	(test(<>(str-compare ?f -)0))
	(test(<>(str-compare ?g -)0))
	(test(<>(str-compare ?h -)0))
	(test(<>(str-compare ?i -)0))
	(test(<>(str-compare ?j -)0))
	
=>
	(printout t "Trebuie sa eliminam linia " ?A  crlf)
	(assert(EliminaLiniePlina ?A (+ ?A 1)))
	(bind ?*LiniiCompletate* (+ ?*LiniiCompletate* 1)) ;; de fiecaredata cand avem o linie plina, incrementam numarul de linii complete cu 1
	(bind ?*scor* (+ ?*scor* (* ?*LiniiCompletate* 10))) 
)

