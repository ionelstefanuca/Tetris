(defclass linie
	(is-a USER) 
	(role concrete) 
	(slot p0 (create-accessor write)(access read-write))
	(slot p1 (create-accessor write)(access read-write))
	(slot p2 (create-accessor write)(access read-write))
	(slot p3 (create-accessor write)(access read-write))
	(slot p4 (create-accessor write)(access read-write))
	(slot p5 (create-accessor write)(access read-write))
	(slot p6 (create-accessor write)(access read-write))
	(slot p7 (create-accessor write)(access read-write))
	(slot p8 (create-accessor write)(access read-write))
	(slot p9 (create-accessor write)(access read-write))
	(slot linieNr (create-accessor write)(access read-write))
)

(deftemplate randomValoare
	(slot random)
)

(defglobal
?*scor* = 0
?*dificultate* = 1
?*nrPieseJoc* = 0   			;numarul de piese selectate pentru joc
?*piesaRandom* = 0
?*ZeroGlobal* = 0
