;trebuie sa generam random piese
;prima data generam un numar...  si dupa alegem din lista de fapte piesa corespunzatoare acestui numar
(defrule GenereazaRandomNumar
(declare (salience 90))
	(genereazaRandomPiese)
	=>
	(bind ?*piesaRandom* (random 1 ?*nrPieseJoc*))
)

(defrule AlegePiesa
(declare (salience 20))
	(genereazaRandomPiese)
    (PiesaSelectata ?a)
	;(test(>= ?*piesaRandom* ?*ZeroGlobal*))
	;(>= ?*piesaRandom* ?*ZeroGlobal*)
	=>
	  (if (> ?*piesaRandom* ?*ZeroGlobal*)
      then
	  ;(printout t "Piesa " ?a " " ?*piesaRandom* crlf )
      ;(printout t "Este mai mare" crlf)
	  (bind ?*piesaRandom* (- ?*piesaRandom* 1))
	  (bind ?*PiesaAleasa* ?a)
	  
      else(printout t""))	
)
(defrule AlegePiesaFact
(declare (salience 15))
	(genereazaRandomPiese)
    (PiesaSelectata ?a)
	=>
	(assert (piesaSeletata ?*PiesaAleasa*))
)


