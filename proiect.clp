; este meniul clasic folosit la laboratorul 2
(defrule meniu 
	(meniu)
	(not(meniu 6))
=>
	(printout t "Afisare meniu: " crlf)
	(printout t "1> Selecteaza piesele care pot fi folosite" crlf)
	(printout t "2> Listare piese selectate" crlf)
	(printout t "3> Seteaza nivelul de dificultate" crlf)
	(printout t "4> Incepe jocul manual" crlf)
	(printout t "5> Afisare tabla" crlf)
	(printout t "6> Incepe jocul automat" crlf)
	(printout t "Introdu optiunea: ")
	(assert (meniu (read)))	
)

